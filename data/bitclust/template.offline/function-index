<table class="entries functions">
<%
    headline_push
    @entries.each do |f|
%>
<tr>
<td class="signature"><%= function_link(f.name) %></td>
<td class="description"><%= compile_rd(f.synopsis_source) %></td>
</tr>
<%
    end
    headline_pop
%>
</table>
